{
    "pages": [
        {
            "path": "pages/list/index"
        },
        {
            "path": "pages/order/index"
        },
        {
            "path": "pages/self/index"
        }
    ],
    "easycom": {
        "autoscan": true,
        "custom": {
            "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
        }
    },
    "tabBar": {
        "color": "#B3ACAC",
        "selectedColor": "#1A1818",
        "borderStyle": "white",
        "backgroundColor": "#F8F8F8",
        "list": [
            {
                "pagePath": "pages/list/index",
                "iconPath": "static/list.png",
                "selectedIconPath": "static/listSelect.png",
                "text": "runners"
            },
            {
                "pagePath": "pages/order/index",
                "iconPath": "static/order.png",
                "selectedIconPath": "static/orderSelect.png",
                "text": "订单"
            },
            {
                "pagePath": "pages/self/index",
                "iconPath": "static/self.png",
                "selectedIconPath": "static/selfSelect.png",
                "text": "我的"
            }
        ]
    },
    "globalStyle": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "black",
        "navigationBarTitleText": "Runners",
        "navigationBarBackgroundColor": "#F8F8F8",
        "backgroundColor": "#F8F8F8"
    }
